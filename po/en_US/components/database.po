#, max-length:370
#. [04485] data/database.json lore/gods/content/page-01-02-square/content #5459
msgid "data/database.json/lore/gods/content/page-01-02-square/content"
msgstr "- \\c[3]Di'aro\\c[0]: God of the Square"

#: data/database.json lore/gods/content/page-01-02-penta/content #5460
#, max-length:150
#. [04486] data/database.json lore/gods/content/page-01-02-penta/content #5460
msgid "data/database.json/lore/gods/content/page-01-02-penta/content"
