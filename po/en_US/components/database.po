#, max-length:380
#. [04487] data/database.json lore/gods/content/page-01-02-penta/altContent/content #5484
#. IF lore.unlocked.godstories-kwin
msgid "data/database.json/lore/gods/content/page-01-02-penta/altContent/content"
msgstr "- \\c[3]K'win\\c[0]: God of the Pentagon"

#: data/database.json lore/gods/content/page-01-02-hexa/content #5461
#, max-length:150
#. [04488] data/database.json lore/gods/content/page-01-02-hexa/content #5461
msgid "data/database.json/lore/gods/content/page-01-02-hexa/content"
