msgctxt "data/maps/jungle/dng/shock-exterior-end.json//entities/132/settings/event/27/elseStep/1/task"
msgid "You are ready to enter the \\c[3]Grand Krys'kajo\\c[0] now!"
msgstr "Теперь вы готовы войти в \\c[3]Гранд Крис'кайо\\c[0]!"

