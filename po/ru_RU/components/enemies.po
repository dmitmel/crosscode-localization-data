msgid "A Flying-type variant of the \\c[3]\\v[combat.name.hedgehog]\\c[0]."
msgstr "Летающий вид \\c[3]Дикоборза\\c[0]."

#. data/enemies/hedgehog.json meta/species #4
#: data/enemies/hedgehog.json+meta/species+%234
