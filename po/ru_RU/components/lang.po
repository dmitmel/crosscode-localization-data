msgctxt "data/lang/sc/gui.en_US.json/labels/skills/descriptions/STATUS_CONDITION_HEALING"
msgid "Increases the recovery speed from all negative status conditions by \\c[3][xyz]%\\c[0]."
msgstr "Увеличивает скорость восстановления от всех негативных эффектов на \\c[3][xyz]%\\c[0]."

#. [10235] data/lang/sc/gui.en_US.json labels/skills/descriptions/HP_REGENERATION #null
