msgid "Decreases time needed to aim by \\c[3][xyz]%\\c[0]."
msgstr "Сокращает время, необходимое для прицеливания, на \\c[3][xyz]%\\c[0]."

#. [10218] data/lang/sc/gui.en_US.json labels/skills/descriptions/AIMING_STABILITY #null
