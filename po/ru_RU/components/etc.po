msgid "Ki Thrust"
msgstr "Выпад Ки"

#. data/players/lea.json actions/WAVE/ATTACK_SPECIAL1_A/description #133
#: data/players/lea.json+actions/WAVE/ATTACK_SPECIAL1_A/description+%23133
msgctxt "data/players/lea.json//actions/WAVE/ATTACK_SPECIAL1_A/description"
