msgctxt "data/maps/jungle/dng/shock-exterior-end.json//entities/139/settings/dialog/2/text"
msgid "P-Please calm down..."
msgstr "P-Por favor, cálmate..."

#. data/maps/jungle/dng/shock-exterior-end.json entities/139/settings/dialog/3/text #916
#. XenoDialog SPAWN IF npc.heightChan == 40 || npc.heightChan == 44
#. cross-worlds.hexacast-female-height-chan
#: data/maps/jungle/dng/shock-exterior-end.json+entities/139/settings/dialog/3/text+%23916
msgctxt "data/maps/jungle/dng/shock-exterior-end.json//entities/139/settings/dialog/3/text"
msgid "The ground...\\. that's the thing that makes you...\\. not fall..."
msgstr "El suelo...\\. es justamente lo que evita que...\\. te caigas..."

#. data/maps/jungle/dng/shock-exterior-end.json entities/139/settings/dialog/4/text #917
#. XenoDialog SPAWN IF npc.heightChan == 40 || npc.heightChan == 44
#. cross-worlds.hexacast-female-height-chan
#: data/maps/jungle/dng/shock-exterior-end.json+entities/139/settings/dialog/4/text+%23917
