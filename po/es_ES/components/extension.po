msgid ""
"\\c[1]INTERNAL MAIL, SENT\\c[0]\n"
"\n"
"\\c[3]FROM:\\c[0] Facility Staff, Office 3A\n"
"\\c[3]TO:\\c[0] \"Pressure\", Management\n"
"\n"
"\\c[3]Subject:\\c[0] Facility Hygiene"
msgstr ""

#. extension/scorpion-robo/data/maps/heat/lab/room-3.json entities/123/settings/condAnims/0/interact/event/2/text #231
