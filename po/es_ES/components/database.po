msgstr ""

#. data/database.json enemies/special.target-bot/descriptions/0/text #-3
#: data/database.json+enemies/special.target-bot/descriptions/0/text+%23-3
msgctxt "data/database.json//enemies/special.target-bot/descriptions/0/text"
msgid "A state-of-the-art bot used to train \\c[3]Seekers\\c[0] in various ways. Word has it that in the early stages of the game the developers based its design off real robots."
msgstr ""

#. data/database.json enemies/mine-runbot/name #316
#: data/database.json+enemies/mine-runbot/name+%23316
msgctxt "data/database.json//enemies/mine-runbot/name"
