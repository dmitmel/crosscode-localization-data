#. data/database.json enemies/autumn-rh.turret_boss-quest_naval/descriptions/0/text #-26
#: data/database.json+enemies/autumn-rh.turret_boss-quest_naval/descriptions/0/text+%23-26
msgctxt "data/database.json//enemies/autumn-rh.turret_boss-quest_naval/descriptions/0/text"
msgid ""
"Heavily modified turrets made in \\c[3]\\v[area.rhombus-sqr.name]\\c[0] and meant for illegal activities all over \\v[lore.title.planet]. They are equipped with \\c[3]floating devices\\c[0] to cross the ocean on their own and might even have their own \\c[3]shielding system\\c[0].\n"
"It is advised to only face them with the support of \\c[3]heavy artillery\\c[0]."
msgstr ""

#. data/database.json enemies/special.target-bot/name #11663
#: data/database.json+enemies/special.target-bot/name+%2311663
msgctxt "data/database.json//enemies/special.target-bot/name"
msgid "Tr4in-0"
msgstr ""

#. data/database.json enemies/special.target-bot/species #-1
#: data/database.json+enemies/special.target-bot/species+%23-1
msgctxt "data/database.json//enemies/special.target-bot/species"
