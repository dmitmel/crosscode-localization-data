msgid "\\c[3]Track of the Ancients\\c[0]"
msgstr "\\c[3]Camino de los Antiguos\\c[0]"

#. [33917] data/maps/rookie-harbor/central-quest-hub-2.json entities/29/settings/condAnims/0/interact/event/0/text #75
#. Prop 02_track
#. IF item.135.amount >= 1
